{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "./APP/scrap.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "./APP/index.html",
    "default_icon": {
      "16": "./APP/image/get_started16.png",
      "32": "./APP/image/get_started32.png",
      "48": "./APP/image/get_started48.png",
      "128": "./APP/image/get_started128.png"
    }
  },
  "icons": {
    "16": "./APP/image/get_started16.png",
    "32": "./APP/image/get_started32.png",
    "48": "./APP/image/get_started48.png",
    "128": "./APP/image/get_started128.png"
  }
}